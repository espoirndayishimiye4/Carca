<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="Carca.Dashboard"
            xmlns:local="clr-namespace:Carca"
            NavigationPage.HasNavigationBar="False"
            BarBackground="white"
            SelectedTabColor="#1c0c65"
            UnselectedTabColor="Coral"
            xmlns:android="clr-namespace:Xamarin.Forms.PlatformConfiguration.AndroidSpecific;assembly=Xamarin.Forms.Core"
            android:TabbedPage.ToolbarPlacement="Bottom"
            
            >

    <local:Home  IconImageSource="home.png"></local:Home>
    <local:Menu  IconImageSource="windows.png"></local:Menu>
    <local:File  IconImageSource="file.png"></local:File>
    <local:Money  IconImageSource="wallet.png"></local:Money>
    <local:Settings IconImageSource="settings.png"></local:Settings>
</TabbedPage>