<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Carca.SignIn"
             
            >
    <ContentPage.Content>
        <StackLayout Padding="30">
            <Label Text="First Name"></Label>

            <Entry FontSize="15"></Entry>
            <Label Text="Last Name"></Label>
            <Entry FontSize="15"></Entry>
            <Label Text="Mobile Phone Number"></Label>
            <Entry FontSize="15"></Entry>
            <Label Text="Secondary Phone Number"></Label>
            <Entry FontSize="15"></Entry>
            <Label Text="I have Read and Agree to The"></Label>
            <Label Text="Terms And Conditions"></Label>
            <Button Text="Continue"
                    Clicked="Button_Clicked"
                    BackgroundColor="#aaaaaa"
                    TextColor="White"
                    CornerRadius="40"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>